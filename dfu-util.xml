<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='interface.xsl'?>
<interface uri="https://raw.githubusercontent.com/kyleparrott/dfu-util/master/dfu-util.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
    <name>dfu-util</name>
    <summary>Flash utility for DFU Devices</summary>
    <homepage>http://dfu-util.sourceforge.net/</homepage>
    <group main="dfu-util">
        <group arch="MacOSX-*">
            <requires importance="required" interface="https://raw.githubusercontent.com/kyleparrott/libusb1.0.0/master/libusb.xml">
                <environment insert="" mode="prepend" name="DYLD_LIBRARY_PATH"/>
            </requires>
            <implementation id="sha1=f85d1e68d7723366c9267c69e4af7d7911d26740" version="0.0">
                <archive href="https://raw.githubusercontent.com/kyleparrott/dfu-util/master/macos/dfu-util.zip" size="20778"/>
            </implementation>
        </group>
        <group arch="Linux-*">
            <requires importance="required" interface="http://127.0.0.1:8000/openocd/libusb1.0.0/libusb.xml">
                <environment insert="" mode="prepend" name="LD_LIBRARY_PATH"/>
            </requires>
            <implementation id="sha1=2107472c1e14b5fb2fca6a23fdaffb15737e254a" version="0.0">
                <archive href="https://raw.githubusercontent.com/kyleparrott/dfu-util/master/linux/dfu-util.zip" size="21670"/>
            </implementation>
        </group>
    </group>
</interface>
